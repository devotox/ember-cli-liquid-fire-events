{
	"name": "ember-cli-liquid-fire-events",
	"version": "1.0.7",
	"description": "Simple events triggered on Liquid Fire animations.",
	"repository": "https://github.com/devotox/ember-cli-liquid-fire-events.git",
	"keywords": [
		"ember-addon"
	],
	"license": "MIT",
	"author": "Devonte Emokpae",
	"directories": {
		"doc": "doc",
		"test": "tests"
	},
	"scripts": {
		"build": "ember build",
		"start": "ember server",
		"test": "ember try:each",
		"postrelease": "npm publish",
		"release": "npm version patch",
		"release-patch": "npm version patch",
		"release-minor": "npm version minor",
		"release-major": "npm version major",
		"postversion": "git push && git push --tags",
		"deploy:release": "npm run release; npm run deploy",
		"deploy:setup": "git add -A; git commit -m \"Added ember-cli-github-pages addon\"; git checkout --orphan gh-pages; rm -rf `bash -c \"ls -a | grep -vE '.gitignore|.git|node_modules|bower_components|(^[.]{1,2}/?$)'\"`; touch .gitkeep; git add -A; git commit -m \"initial gh-pages commit\"; git checkout master",
		"deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push origin gh-pages; git checkout master"
	},
	"dependencies": {
		"ember-cli-babel": "^6.6.0",
		"ember-cli-htmlbars": "^2.0.1"
	},
	"devDependencies": {
		"broccoli-asset-rev": "^2.4.5",
		"ember-ajax": "^3.0.0",
		"ember-cli": "~2.16.0-beta.1",
		"ember-cli-dependency-checker": "^2.0.0",
		"ember-cli-eslint": "^4.0.0",
		"ember-cli-htmlbars": "^2.0.1",
		"ember-cli-github-pages": "^0.1.2",
		"ember-cli-htmlbars-inline-precompile": "^1.0.0",
		"ember-cli-inject-live-reload": "^1.4.1",
		"ember-cli-qunit": "^4.0.0",
		"ember-cli-shims": "^1.1.0",
		"ember-cli-sri": "^2.1.0",
		"ember-cli-uglify": "^2.0.0",
		"ember-disable-prototype-extensions": "^1.1.2",
		"ember-export-application-global": "^2.0.0",
		"ember-load-initializers": "^1.0.0",
		"ember-resolver": "^4.0.0",
		"ember-source": "~2.16.0-beta.1",
		"liquid-fire": "^0.28.0",
		"loader.js": "^4.2.3"
	},
	"engines": {
		"node": "^4.5 || 6.* || >= 7.*"
	},
	"ember-addon": {
		"configPath": "tests/dummy/config",
		"demoURL": "https://devotox.github.io/ember-cli-liquid-fire-events"
	}
}
